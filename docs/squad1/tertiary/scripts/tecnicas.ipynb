df = pd.DataFrame(detalhes_lista)
df = df.sort_values(by=['Cat', 'Freq'], ascending=[True, True])

# --- 2. VISUAL ---
cores_map = {
    'Avaliação Humana': '#4e79a7',
    'Métricas Quantitativas': '#f28e2b',
    'Testes Automatizados': '#59a14f',
    'Benchmarks & Datasets': '#e15759'
}

fig = plt.figure(figsize=(12, 17))
gs = GridSpec(2, 1, height_ratios=[1, 2.7], hspace=0.3)

# --- 3. PIZZA ---
ax_pie = fig.add_subplot(gs[0])
labels = list(dados_pizza.keys())
values = list(dados_pizza.values())
colors = [cores_map[l] for l in labels]

wedges, texts, autotexts = ax_pie.pie(
    values, labels=labels, autopct='%1.1f%%', 
    startangle=140, colors=colors, explode=(0.03,)*4, 
    textprops={'fontsize': 11, 'weight': 'bold'}
)
plt.setp(autotexts, size=11, weight="bold", color="white") 
for w in wedges: w.set_edgecolor('white'), w.set_linewidth(1)
ax_pie.set_title("Técnicas de Avaliação", fontsize=16, fontweight='bold')

# --- 4. BARRAS ---
ax_bar = fig.add_subplot(gs[1])
colors_bar = [cores_map[cat] for cat in df['Cat']]
bars = ax_bar.barh(df['Técnica'], df['Freq'], color=colors_bar, height=0.7)

ax_bar.set_title("Detalhamento: Ferramentas", fontsize=16, fontweight='bold', pad=15)
ax_bar.set_xlabel("Frequência Absoluta de Citações nos Estudos", fontsize=12)

for rect in bars:
    width = rect.get_width()
    ax_bar.text(width + 0.2, rect.get_y() + rect.get_height()/2, f'{int(width)}', 
                ha='left', va='center', fontsize=10, fontweight='bold')

ax_bar.spines['top'].set_visible(False)
ax_bar.spines['right'].set_visible(False)
ax_bar.grid(axis='x', linestyle='--', alpha=0.3)

handles = [plt.Rectangle((0,0),1,1, color=cores_map[c]) for c in cores_map]
ax_bar.legend(handles, cores_map.keys(), title="Legenda de Cores", loc="lower right")

plt.tight_layout()
plt.show()


# --- Cédula 2 ---
import matplotlib.pyplot as plt
import seaborn as sns
import pandas as pd
import numpy as np

# --- 1. DADOS ---
categories_data = {
    "Avaliação Humana": ["A16", "A17", "A21", "A22", "A24", "A27", "A34", "A36", "A37", "A38", "A41", "A45", "A50", "A51", "A55", "A58", "A60", "A61", "A64", "A65", "A66", "A68"],
    "Métricas Quantitativas": ["A1", "A6", "A10", "A12", "A19", "A21", "A22", "A24", "A27", "A36", "A38", "A44", "A50", "A54", "A55", "A62", "A65", "A68"],
    "Testes Automatizados": ["A5", "A6", "A7", "A16", "A17", "A19", "A21", "A27", "A36", "A37", "A44", "A51", "A54", "A57", "A58", "A59", "A62"],
    "Benchmarks": ["A1", "A6", "A17", "A22", "A27", "A41", "A44", "A57", "A58", "A60", "A62", "A65", "A66"],
    "Revisão Sistemática": ["A9", "A30", "A54"]
}

# --- 2. PROCESSAMENTO ---
all_studies = sorted(list({study for sublist in categories_data.values() for study in sublist}))

data_matrix = []
for study in all_studies:
    row = {cat: (1 if study in studies else 0) for cat, studies in categories_data.items()}
    data_matrix.append(row)

df = pd.DataFrame(data_matrix)
cooc = df.T.dot(df)

order = cooc.sum().sort_values(ascending=False).index
cooc = cooc.loc[order, order]

# --- 3. PLOTAGEM LIMPA ---
sns.set_style("white") 
plt.figure(figsize=(10, 8))

mask = np.triu(np.ones_like(cooc, dtype=bool), k=0)

ax = sns.heatmap(
    cooc,
    mask=mask,
    cmap="mako",
    annot=cooc.where(~mask),
    fmt=".0f",
    linewidths=0, 
    square=True,
    cbar_kws={"label": "Número de Estudos em Comum", "shrink": 0.8},
    annot_kws={"size": 11}
)

ax.set_title("Heatmap de Coocorrência: Técnicas de Avaliação", fontsize=16, pad=20)

labels = list(cooc.columns)
n = len(labels)

ax.set_xticks(np.arange(0, n-1) + 0.5)
ax.set_xticklabels(labels[:-1], rotation=45, ha="right", fontsize=11)

ax.set_yticks(np.arange(1, n) + 0.5)
ax.set_yticklabels(cooc.index[1:], fontsize=11, rotation=0)

sns.despine(left=True, bottom=True)

plt.tight_layout()
plt.show()
